{
  "hosting": {
    "public": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "android.keystore",
      "app/**",
      "gradle/**",
      "*.gradle",
      "gradlew",
      "gradlew.bat",
      "local.properties",
      "*.md",
      "release-notes-formatted.txt"
    ],
    "headers": [
      {
        "source": "/.well-known/assetlinks.json",
        "headers": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ]
      }
    ]
  }
}
